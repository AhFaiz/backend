#

print("=== Program 1 (Loop Trough The Tuple) ===")

thistuple = ("apple", "banana", "cherry")
for x in thistuple:
  print(x)

#=========================================

print("\n=== Program 2 (Lopp Trough The Index Number) ===")

thistuple = ("banana", "apple", "cherry")
for i in range(len(thistuple)):
  print(thistuple[i])